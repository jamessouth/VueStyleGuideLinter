{
  "name": "vue-style-guide-linter",
  "version": "1.0.0",
  "description": "A Vue SFC style linter using the Vue Style Guide",
  "module": "./src/js/main.js",
  "bin": "./bin/vue-style-guide-linter",
  "scripts": {
    "lint": "eslint ./bin ./src ./test",
    "runme": "node ./bin/vue-style-guide-linter.js",
    "test": "nyc --reporter=lcov ava \"test/test.js\" --verbose && nyc report"
  },
  "engines": {
		"node": ">=10.0.0"
	},
  "keywords": [
    "vue",
    "style",
    "guide",
    "lint",
    "linter",
    "sfc",
    "vue-cli"
  ],
  "contributors": [
    {
      "name": "James South",
      "website": "https://jamessouth.github.io/Project-12/"
    }
  ],
  "files": [
    "src",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/vue-style-guide-linter.git"
  },
  "author": "James South <jamesdanielsouth@gmail.com>",
  "license": "MIT",
  "bugs": "https://github.com/jamessouth/vue-style-guide-linter/issues",
  "homepage": "https://github.com/jamessouth/vue-style-guide-linter#readme",
  "dependencies": {
    "abstract-syntax-tree": "^2.1.0",
    "esm": "^3.2.0"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-ava": "^5.1.1",
    "eslint-plugin-import": "^2.16.0",
    "nyc": "^13.2.0",
    "sinon": "^7.2.3"
  },
  "ava": {
    "require": "esm"
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "test/**",
      "src/**/pipe.js",
      "src/**/getComponentProperties.js",
      "**/node_modules/**"
    ]
  }
}
