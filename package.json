{
  "name": "vue-style-guide-linter",
  "version": "1.0.0",
  "description": "A Vue SFC style linter using the Vue Style Guide",
  "module": "./src/js/main.js",
  "scripts": {
    "lint": "eslint ./VueStyleGuideLinter.js ./src",
    "start": "node --no-warnings ./VueStyleGuideLinter.js",
    "test": "nyc --reporter=lcov ava \"tests/test.js\" --verbose && nyc report"
  },
  "keywords": ["vue", "style", "guide", "lint", "linter", "sfc", "vue-cli"],
  "contributors": [
    {
      "name": "James South",
      "website": "https://jamessouth.github.io/Project-12/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/VueStyleGuideLinter.git"
  },
  "author": "James South <jamesdanielsouth@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamessouth/VueStyleGuideLinter/issues"
  },
  "homepage": "https://github.com/jamessouth/VueStyleGuideLinter#readme",
  "dependencies": {
    "abstract-syntax-tree": "^2.1.0",
    "esm": "^3.2.0"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nyc": "^13.2.0",
    "sinon": "^7.2.3"
  },
  "ava": {
    "require": "esm"
  },
  "nyc": {
    "exclude": ["coverage/**", "tests/**", "src/**/pipe.js", "src/**/getComponentProperties.js", "**/node_modules/**"]
  }
}
